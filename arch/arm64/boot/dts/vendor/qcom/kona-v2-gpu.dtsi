&soc {
	msm_bus: qcom,kgsl-busmon {
		label = "kgsl-busmon";
		compatible = "qcom,kgsl-busmon";
		operating-points-v2 = <&gpu_opp_table_v2>;
	};

	gpu_opp_table_v2: gpu-opp-table_v2 {
		compatible = "operating-points-v2";

		opp-925000000 {
			opp-hz = /bits/ 64 <925000000>;
			opp-microvolt = <397>;
		};

		opp-840000000 {
			opp-hz = /bits/ 64 <840000000>;
                       opp-microvolt = <269>;
		};

		opp-712000000 {
			opp-hz = /bits/ 64 <712000000>;
                       opp-microvolt = <246>;
		};

		opp-613000000 {
			opp-hz = /bits/ 64 <613000000>;
                       opp-microvolt = <214>;
		};

		opp-511900000 {
			opp-hz = /bits/ 64 <511900000>;
                       opp-microvolt = <167>;
		};

		opp-441600000 {
			opp-hz = /bits/ 64 <441600000>;
                       opp-microvolt = <82>;
		};

		opp-380000000 {
			opp-hz = /bits/ 64 <380000000>;
                       opp-microvolt = <32>;
		};

		opp-325000000 {
			opp-hz = /bits/ 64 <325000000>;
                       opp-microvolt = <1>;
		};

		opp-267000000 {
			opp-hz = /bits/ 64 <267000000>;
                       opp-microvolt = <1>;
		};

	};
};

&msm_gpu {
	qcom,chipid = <0x06050001>;

	/* GPU OPP data */
	operating-points-v2 = <&gpu_opp_table_v2>;

	/delete-property/qcom,initial-pwrlevel;
	/delete-node/qcom,gpu-pwrlevels;

	/* Power levels bins */
	qcom,gpu-pwrlevel-bins {
		compatible="qcom,gpu-pwrlevel-bins";
		#address-cells = <1>;
		#size-cells = <0>;

		qcom,gpu-pwrlevels-0 {
			#address-cells = <1>;
			#size-cells = <0>;
			qcom,speed-bin = <0>;
			qcom,initial-pwrlevel = <8>;
			qcom,throttle-pwrlevel = <0>;

                        qcom,gpu-pwrlevel@0 {
                                reg = <0>;
                                qcom,gpu-freq = <925000000>;
                                qcom,bus-freq-ddr7 = <1>;
				qcom,bus-min-ddr7 = <1>;
				qcom,bus-max-ddr7 = <11>;
                                qcom,bus-freq-ddr8 = <1>;
				qcom,bus-min-ddr8 = <1>;
				qcom,bus-max-ddr8 = <11>;
                                qcom,acd-level = <0x802b5ffd>;
                        };

                        qcom,gpu-pwrlevel@1 {
                                reg = <1>;
                                qcom,gpu-freq = <840000000>;
                                qcom,bus-freq-ddr7 = <1>;
                                qcom,bus-min-ddr7 = <1>;
                                qcom,bus-max-ddr7 = <11>;
                                qcom,bus-freq-ddr8 = <1>;
                                qcom,bus-min-ddr8 = <1>;
                                qcom,bus-max-ddr8 = <11>;
                                qcom,acd-level = <0xaa5e9ffd>;
                        };

                        qcom,gpu-pwrlevel@2 {
                                reg = <2>;
                                qcom,gpu-freq = <712000000>;
                                qcom,bus-freq-ddr7 = <1>;
                                qcom,bus-min-ddr7 = <1>;
                                qcom,bus-max-ddr7 = <11>;
                                qcom,bus-freq-ddr8 = <1>;
                                qcom,bus-min-ddr8 = <1>;
                                qcom,bus-max-ddr8 = <11>;
                                qcom,acd-level = <0xaa5e9ffd>;
                        };

			qcom,gpu-pwrlevel@3 {
				reg = <3>;
				qcom,gpu-freq = <613000000>;
                                qcom,bus-freq-ddr7 = <1>;
                                qcom,bus-min-ddr7 = <1>;
                                qcom,bus-max-ddr7 = <11>;
                                qcom,bus-freq-ddr8 = <1>;
                                qcom,bus-min-ddr8 = <1>;
                                qcom,bus-max-ddr8 = <11>;
				qcom,acd-level = <0xaa3e7ffd>;
			};

			qcom,gpu-pwrlevel@4 {
				reg = <4>;
				qcom,gpu-freq = <511900000>;
				qcom,bus-freq-ddr7 = <1>;
				qcom,bus-min-ddr7 = <1>;
				qcom,bus-max-ddr7 = <6>;

				qcom,bus-freq-ddr8 = <1>;
				qcom,bus-min-ddr8 = <1>;
				qcom,bus-max-ddr8 = <6>;

				qcom,acd-level = <0xaa3e7ffd>;
			};

			qcom,gpu-pwrlevel@5 {
				reg = <5>;
				qcom,gpu-freq = <441600000>;
				qcom,bus-freq-ddr7 = <1>;
				qcom,bus-min-ddr7 = <1>;
				qcom,bus-max-ddr7 = <6>;

				qcom,bus-freq-ddr8 = <1>;
				qcom,bus-min-ddr8 = <1>;
				qcom,bus-max-ddr8 = <6>;

				qcom,acd-level = <0xaa2e6ffd>;
			};

			qcom,gpu-pwrlevel@6 {
				reg = <6>;
				qcom,gpu-freq = <380000000>;
				qcom,bus-freq-ddr7 = <1>;
				qcom,bus-min-ddr7 = <1>;
				qcom,bus-max-ddr7 = <5>;

				qcom,bus-freq-ddr8 = <1>;
				qcom,bus-min-ddr8 = <1>;
				qcom,bus-max-ddr8 = <5>;

				qcom,acd-level = <0xaa2e6ffd>;
			};

			qcom,gpu-pwrlevel@7 {
				reg = <7>;
				qcom,gpu-freq = <325000000>;
				qcom,bus-freq-ddr7 = <1>;
				qcom,bus-min-ddr7 = <1>;
				qcom,bus-max-ddr7 = <5>;

				qcom,bus-freq-ddr8 = <1>;
				qcom,bus-min-ddr8 = <1>;
				qcom,bus-max-ddr8 = <5>;

				qcom,acd-level = <0xaa1e5ffd>;
			};

			qcom,gpu-pwrlevel@8 {
				reg = <8>;
				qcom,gpu-freq = <267000000>;
				qcom,bus-freq-ddr7 = <1>;
				qcom,bus-min-ddr7 = <1>;
				qcom,bus-max-ddr7 = <5>;

				qcom,bus-freq-ddr8 = <1>;
				qcom,bus-min-ddr8 = <1>;
				qcom,bus-max-ddr8 = <5>;

				qcom,acd-level = <0xaa1e5ffd>;
			};

			qcom,gpu-pwrlevel@9 {
				reg = <9>;
				qcom,gpu-freq = <0>;
				qcom,bus-freq = <0>;
				qcom,bus-min = <0>;
				qcom,bus-max = <0>;
			};
		};
	};
};
